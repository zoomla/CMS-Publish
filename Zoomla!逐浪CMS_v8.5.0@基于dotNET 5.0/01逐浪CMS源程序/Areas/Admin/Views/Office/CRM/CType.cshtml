@model System.Data.DataTable
@section head{<title>@L.客户类别</title>}
@{
Layout = "~/Areas/Admin/Views/Shared/_layout.cshtml";
        HttpRequest Request = Context.Request;
}
@section content{
<div class="w-100">
    <ol id="navWrap" class="breadcrumb fixed-top w-100" style="padding:.75rem 1rem;">
        <li class="breadcrumb-item"><a href="@(CustomerPageAction.customPath2)I/Main">@L.工作台</a></li>
        <li class="breadcrumb-item">
            <a href="@MvcHtmlString.Create(Request.RawUrl())">@L.客户类别</a>
            [<a href="CTypeAdd">@L.添加类别</a>]
        </li>
    </ol>
    @Html.Partial("CType_List", Model)
    <div class="sysBtline">
        <input type="button" value="@L.批量删除" class="btn btn-outline-danger" onclick="mvcpage.bat({url: 'client_del', confirm: '@Html.Raw(L.确定要批量移除吗)' });" />
    </div>
</div>
}
@section script{
    <script src="/JS/Controls/ZL_Dialog.js"></script>
}