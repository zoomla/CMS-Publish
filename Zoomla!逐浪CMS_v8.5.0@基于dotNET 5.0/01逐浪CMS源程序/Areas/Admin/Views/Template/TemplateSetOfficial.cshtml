@model ZoomLa.SQLDAL.SQL.PageSetting

@{


}
@section head{
<title>@L.获取云模板</title> 
    <style type="text/css">
    .pagination{justify-content: center;}
    </style>
    }
@section content{
@Call.SetBread(new Bread[] {
new Bread("{admin}",L.工作台),
new Bread("TemplateSet",L.模板风格),
new Bread(){url="templateSetAdd",text="["+L.发布当前站点方案+"]",addon="<a href='#' data-bs-toggle='modal' data-bs-target='#myModal1'>["+L.使用前必读+"]</a>" }
})
<div class="template">
<div class="panel panel-default">
<div class="panel-body">
<h1><i class="zi zi_cloud" zico="云"></i> @L.模板云 <a title="免费云建站" target="_blank" href="//site.73ic.com" class="btn btn-outline-info">->@L.免费在线拖拽式云建站</a>
</h1>
<div class="template sys_Template">
<div class="modal fade" id="myModal1" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
<div class="modal-dialog modal-dialog-centered">
	<div class="modal-content">
		<div class="modal-header">
			<h4 class="modal-title" id="myModalLabel">@L.免费云方案下载</h4>
			<button type="button" class="btn-close" data-bs-dismiss="modal" aria-hidden="true"></button>
		</div>
		<div class="modal-body">
			<p>
				@L.在线方案_提示_免费<br>
				@L.在线方案_提示_完整<br>
				@L.在线方案_提示_下载
			</p>
		</div>
		<div class="modal-footer">
			<button type="button" class="btn btn-default" data-bs-dismiss="modal">Close</button>
		</div>
	</div>
</div>
</div>
   
@Html.Partial("TemplateSetOfficial_List")

</div>
</div>

}
@section script{

<link href="/Plugins/JqueryUI/LightBox/css/lightbox.css" rel="stylesheet" media="screen" />
<script src="/Plugins/JqueryUI/LightBox/jquery.lightbox.js" type="text/javascript"></script>
<script>
$(document).ready(function () {
base_url = document.location.href.substring(0, document.location.href.indexOf('index.html'), 0);
$(".lightbox").lightbox({
	fitToScreen: true,
	imageClickClose: false
});
});
</script>

}
</div>
</div>