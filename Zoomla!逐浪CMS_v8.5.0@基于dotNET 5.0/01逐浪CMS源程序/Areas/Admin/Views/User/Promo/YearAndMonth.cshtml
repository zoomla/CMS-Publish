<style type="text/css">
.btn-light {border:1px solid #ddd;}
.btn-light.active {background-color:#0094ff;color:#fff;}
</style>
<div>
    <div class="dashed d-flex flex-wrap">
        <strong>年份：</strong>
        <div class="btn-group d-flex flex-wrap" id="year_div">
            @for (int i = 0; i < 10; i++)
            {
                <a href="javascript:;" onclick="" class="btn btn-light">全部</a>
            }
        </div>
    </div>
    <div class="dashed d-flex flex-wrap">
        <strong>月份：</strong>
        <div class="btn-group d-flex flex-wrap" id="month_div">
            <a href="javascript:;" onclick="" class="btn btn-light">全部</a>
            @for (int i = 0; i < 12; i++)
            {
               <a href="javascript:;" onclick="" class="btn btn-light">@((DateTime.Now.Year - i))月</a>
            }
        </div>
    </div>
</div>
<script>
    $("#year_div .btn,#month_div .btn").click(function () {
        var $this = $(this);
        if ($this.hasClass("active")) { return; }
        $(this).addClass("active");
        $(this).siblings.removeClass("active");
    });
</script>