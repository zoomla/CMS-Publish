@model ZoomLa.Model.M_Grade
@{ 
    M_GradeCate cateMod = ViewBag.cateMod;
    M_Grade parentMod = ViewBag.parentMod;

}
@section head{<title>字典选项</title> }
@section content{ 
<form method="post" action="@MVCHelper.GetAction("GOptionAdd_Submit",Context.Request)">
<table class="table table-striped table-bordered sys_table">
    <tr><th colspan="2" class="text-center">维护[@cateMod.CateName]字典</th></tr>
    <tr>
        <th>上级选项</th>
        <td>@parentMod.GradeName</td>
    </tr>
    <tr>
        <th class="tdbgleft text-end">分级选项：</th>
        <td>
            @Html.TextBoxFor(m=>m.GradeName,new {@class="form-control" })
        </td>
    </tr>
    <tr>
		<td></td>
        <td>
            <input type="submit" ID="btnSave" value="保存信息" class="btn btn-outline-info" data-enter="3"/>
        </td>
    </tr>
</table> 
</form>


}
@section script{ 


}