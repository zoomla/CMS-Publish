@{

}
@section head{ <title>配置中心</title> }
@section content{
<div id="BreadDiv" class="container-fluid mysite">
    <div class="row">
        <ol class="breadcrumb w-100" style="padding:.75rem 1rem;">
            <li class="breadcrumb-item"><a href='@(CustomerPageAction.customPath2)I/main'>@L.工作台</a></li>
            <li class="breadcrumb-item"><a href='Papers_System_Manage'>@L.教育模块</a></li>
            <li class="breadcrumb-item"><a href='QuestionManage'>@L.考试管理</a></li>
            <li class="breadcrumb-item">@L.配置中心</li>
        </ol>
    </div>
</div>
<form method="post" action="Setting_Update" class="w-100">
	<div class="container-fluid pe-0">
	<div class="row sysRow table-responsive">
    <table class="table table-bordered table-striped">
        <tr>
            <td class="w12rem_lg">@L.难度更新</td>
            <td>
                <div class="input-group">
                    <input type="text" id="stime" name="stime" value="@ViewBag.stime" class="form-control max20rem" onclick="WdatePicker({ maxDate: '#F{$dp.$D(\'etime\')}', dateFmt: 'yyyy-MM-dd HH:mm' });" />
                    <input type="text" id="etime" name="etime" value="@ViewBag.etime" class="form-control max20rem" onclick="WdatePicker({ minDate: '#F{$dp.$D(\'stime\')}', dateFmt: 'yyyy-MM-dd HH:mm' });" />
                    <button type="submit" class="btn btn-outline-secondary">@L.难度更新</button>
                </div>
				
				
			  
            </td>
        </tr>
    </table>
	</div>
	</div>
</form>
}
@section script{
<script src="/JS/DatePicker/WdatePicker.js"></script>
}