}
@section head{
<title>学校浏览</title>
}
@section content{

<div id="pageflag" data-nav="edu" data-ban="ke"></div>
<div class="container">
<nav aria-label="breadcrumb" role="navigation">
<ol class="breadcrumb">
	<li class="breadcrumb-item"><a href="@Url.Action("Default","")">用户中心</a></li>
	<li class="breadcrumb-item"><a href="@Url.Action("ClassManage","Exam")">班级管理</a></li>
	<li class="breadcrumb-item active">学校浏览</li>
</ol>
</nav>
</div>
<div class="container btn_green">
    <table class="table table-striped table-bordered table-hover">
        <tr>
            <td class="w_10rem text-end">学校名称:</td>
            <td>
                <label id="schoolname_l"> </label>
            </td>
        </tr>
        <tr>
            <td class="text-end">所属省市:</td>
            <td>
                <label id="schooladdress_l" ></label>
            </td>
        </tr>
        <tr>
            <td class="text-end">学校类型:</td>
            <td>
                <label id="SchoolType_L" ></label>
            </td>
        </tr>
        <tr>
            <td class="text-end">学校性质:</td>
            <td>
                <label id="schooldis_l" ></label>
            </td>
        </tr>
        <tr>
            <td class="text-end">学校微标:</td>
            <td>
                <lasp:Label id="schoolicon_l" ></lasp:Label>
            </td>
        </tr>
        <tr>
            <td class="text-end">学校信息:</td>
            <td>
                <label id="schoolinfo_l" ></label>
            </td>
        </tr>
    </table>
    <div class="text-center"><a href="@Url.Action("ClassManage","Exam")" class="btn btn-outline-info">返回列表</a></div>
</div>
<script>
//导航处理
	$("#usernav1 ul li").removeClass("active").siblings().eq(3).addClass("active");
</script>	
}
@section script{

}
