	</ol>
	</nav>
	</div>
    <div class="container">
       @Html.Partial("/Areas/User/Views/Message/MessageTop.cshtml")
    </div>
    <div class="container btn_green margin_t10">
        @Html.Partial("/Areas/User/Views/Message/MessageRecycle_List.cshtml", Model)
        <div>
            <button id="batdel_btn" class="btn btn-outline-danger" onclick="mvcpage.bat({ url: 'message_RealDel', confirm: '你确定要将所选中的项删除吗' });">批量删除</button>
