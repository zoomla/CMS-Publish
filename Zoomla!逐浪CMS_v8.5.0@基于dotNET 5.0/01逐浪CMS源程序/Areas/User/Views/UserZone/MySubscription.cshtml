@using System.Data
@using ZoomLa.BLL.Helper
@model ZoomLa.SQLDAL.SQL.PageSetting
@{
    Model.cols=5;
    Model.Check=false;
    
}
@section head{
    <title>订阅管理</title>
}
@section content{
    <div id="pageflag" data-nav="group" data-ban="sub"></div>
	<div class="container">
	<nav aria-label="breadcrumb" role="navigation">
	<ol class="breadcrumb">
		<li class="breadcrumb-item"><a title="会员中心" href="@Url.Action("Index","")">会员中心</a></li>
		<li class="breadcrumb-item active">订阅管理</li>
	</ol>
	</nav>
	</div>
   <div class="container margin_t10 btn_green">
    <table class="table table-striped table-bordered">
        <tr>
            <td class="td_s">编号</td>
            <td>订阅标题</td>
            <td class="td_l">生效日期</td>
            <td class="td_l">到期日期</td>
            <td class="td_l">操作</td>
        </tr>
        @foreach (DataRow dr in Model.dt.Rows)
        {
            <tr>
                <td>@dr["Id"]</td>
                <td>
                    <a href="javascript:;">@dr["title"]</a>
                </td>
                <td>@DateHelper.ToDate(dr["effectTime"])</td>
                <td>@DateHelper.ToDate(dr["endTime"])</td>
                <td>
                    <a href="javascript:;" onclick="mvcpage.del({url:'subscript_del',ids:'@dr["ID"]',confirm:'确定要删除吗'});">删除</a>
                </td>
            </tr>
        }
        @Html.Partial("_page", Model)
    </table>
</div>
}
@section script{ }