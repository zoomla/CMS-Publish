@model ZoomLa.SQLDAL.SQL.PageSetting
@{
    
}
@section head{
<script src="/JS/DatePicker/WdatePicker.js"></script>
<title>消费详情</title>
}
@section content{
<div id="pageflag" data-nav="home" data-ban="UserInfo"></div>
<div class="container">
<nav aria-label="breadcrumb" role="navigation">
<ol class="breadcrumb">
	<li class="breadcrumb-item"><a title="会员中心" href="@Url.Action("index","")">会员中心</a></li>
	<li class="breadcrumb-item active"><a href="@Context.Request.RawUrl()">资产变更详情</a></li>
</ol>
</nav>
</div>
<div class="container">
<div class="input-group">
    <input type="text" id="stime_t" placeholder="起始时间" class="form-control text_md mvcparam" onclick="WdatePicker({})" />
    <input type="text" id="etime_t" placeholder="结束时间" class="form-control text_md mvcparam" onclick="WdatePicker({})" />
    <input type="text" id="skey_t" placeholder="关键词" class="form-control text_md mvcparam"/>
    <input type="hidden" id="stype" class="mvcparam" value="@Context.Request.GetParam("SType")" />
    <span class="input-group-append">
        <button type="button" id="Skey_Btn" class="btn btn-outline-secondary" onclick="mvcpage.load();"><span class="zi zi_search"></span></button>
    </span>
</div>
<div class="clearfix margin_t5"></div>
@Html.Partial("ConsumeDetail_List", Model)
</div>
}
@section script{
<style>.allchk_l{display:none;}</style>
}
