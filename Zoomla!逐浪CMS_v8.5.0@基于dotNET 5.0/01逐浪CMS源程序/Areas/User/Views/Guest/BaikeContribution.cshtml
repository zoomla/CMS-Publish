@model ZoomLa.SQLDAL.SQL.PageSetting
@section head{<title>词条贡献</title>}
@section content{
<div id="pageflag" data-nav="index" data-ban="baike"></div>
<div class="container">
<nav aria-label="breadcrumb" role="navigation">
<ol class="breadcrumb">
	<li class="breadcrumb-item"><a title="会员中心" href="@Url.Action("Index","")">会员中心</a></li>
	<li class="breadcrumb-item active">我的词条贡献</li>
	<div class="clearfix"></div>
</ol>
</nav>
</div>

<div class="container">
    <div class="input-group">
		<span class="input-group-text" id="basic-addon1">状态</span>
        <select class="form-control text_150 mvcparam form-select" id="status" onchange="mvcpage.load();">
            <option value="1">已通过</option>
            <option value="0">待审核</option>
            <option value="-2">未通过</option>
            <option value="-100" selected="selected">全部</option>
        </select>
        <input type="text" id="skey" class="form-control text_md mvcparam" placeholder="关键词"/>
        <input type="button" value="搜索" class="btn btn-outline-secondary" onclick="mvcpage.load();" /> @*OnClick="Skey_Btn_Click"*@ 
    </div>
    @Html.Partial("BaikeContribution_List",Model)
</div>
}
