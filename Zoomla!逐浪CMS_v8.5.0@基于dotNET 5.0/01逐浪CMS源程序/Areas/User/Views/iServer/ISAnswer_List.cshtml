@using System.Data
@model ZoomLa.SQLDAL.SQL.PageSetting
@{
    Model.cols = 9;
}
<table id="EGV" class="table table-striped table-bordered table-hover">
    <tr align="center" class="title" style="height: 25px">
        <td>编号</td>
        <td>标题</td>
        <td>用户名</td>
        <td>用户类型</td>
        <td>优先级</td>
        <td>问题类型</td>
        <td>提交时间</td>
        <td>状态</td>
        <td>操作</td>
    </tr>
    @foreach (DataRow dr in Model.dt.Rows)
    {
        <tr align="center">
            <td>@dr["QuestionId"]</td>
            <td><a href="FiServerInfo?ID=@dr["questionid"]">@dr["Title"]</a></td>
            <td>@dr["UserName"]</td>
            <td>@dr["GroupName"]</td>
            <td>@dr["Priority"]</td>
            <td>@dr["Type"]</td>
            <td>@dr["SubTime"]</td>
            <td><label id="lblstate">@dr["State"]</label></td>
            <td>
                <a href="" class="option_style"><i class="zi zi_comment"></i>回复</a>
            </td>
        </tr>
    }
    @Html.Partial("_page", Model)
</table>

