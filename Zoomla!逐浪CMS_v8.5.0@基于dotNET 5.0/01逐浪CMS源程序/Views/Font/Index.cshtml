const well = { template: '#well' }
const myfont = { template: '#myfont' }
const pay = { template: '#pay' }
const help = { template: '#help' }
const routes = [
  { path: '/', component: well },
  { path: '/myfont', component: myfont },
  { path: '/pay', component: pay },
  { path: '/help', component: help }
]
const router = new VueRouter({
  routes // (缩写) 相当于 routes: routes
})
const app = new Vue({
  router
}).$mount('#app')

function deleteFont(id) {
var a = window.confirm("您确定要删除该字体吗？这将删除该字体中所有书写过的字。\n删除请点击确认，否则点击取消。");
if (!a)
    return;
$.ajax({
    method: "GET",
    dataType: "json",
    url: encodeURI('@Url.Action("Delete")?id=' + id),
    success: function (result) {
        if (result.status) {
            alert("字体删除成功！");
            window.location.reload();
        }
        else {
            alert("字体删除失败，错误消息为：" + result.error);
        }
    },
    error: function () {
        alert("出现错误！可能是网络或api存在问题");
    }
});
}
function viewDetail(id) {
