@functions{
    string GetFontType(int type)
    {
        switch (type)
        {
            case 0:
                return "英文字母符号集（共178字）";
            case 1:
                return "小学语文350生字及字母（共457字）";
            case 2:
                return "《现代汉语常用字表》中常用汉字2500字和次常用汉字1000字集合及英文字符（共3007字）";
            case 3:
                return "GB2312国标规范6791汉字及英文字母符号（共6880字）";
            default:
                return "未知字体";
        }
    }
}
