@model ZoomLa.SQLDAL.SQL.PageSetting
@{
string BType=ViewBag.BType;
Layout = "~/Views/FrontBaike/_BaikeLayout.cshtml";
}
@section head{ <title>搜索词条-@(Call.SiteName)百科</title>}
@section content{
<div class="container">
<nav class="mt-3 baike_miannav" aria-label="breadcrumb" role="navigation">
 <ol class="breadcrumb">
	<li class="breadcrumb-item"><a href="/">网站首页</a></li>
	<li class="breadcrumb-item"><a href="/Baike" target="_self">百科中心</a></li>
	<li class="breadcrumb-item" aria-current="page"><a href="CreateBaike">创建词条</a></li>
</ol>
</nav>
<div>@MvcHtmlString.Create(ViewBag.result)</div>
@Html.Partial("Search_List",Model)
</div>
}
@section script{
<script>
$(function () {
var btype = "@BType";
$("#top_nav_ul li[title='"+btype+"']").addClass("active");
})
</script>
}