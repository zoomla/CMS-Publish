@using System.Data
@using ZoomLa.BLL
@model ZoomLa.SQLDAL.SQL.PageSetting
@functions{
    B_IServer isBll = new B_IServer();
}
@{
  Model.cols = 9;
}
<table id="EGV" class="table table-striped table-bordered">
            <tr class="title" style="text-align: center">
                <td>编号</td>
                <td>标题</td>
                <td>优先级</td>
                <td>问题类型</td>
                <td>已读次数</td>
                <td>提交时间</td>
                <td>状态</td>
            </tr>
            @foreach (DataRow dr in Model.dt.Rows)
                        {
                <tr class="text-center">
                    <td>@dr["QuestionId"]</td>
                    <td>
                        <a href="FiServerInfo?ID=@dr["QuestionId"]">@ZoomLa.Common.BaseClass.Htmlcode(@dr["Title"].ToString())</a>
                    </td>
                    <td>@dr["Priority"]</td>
                    <td><a href='SelectiServer?type=@(isBll.GetTypeIndex(dr["type"].ToString()))' target="_self">@dr["type"]</a></td>
                    <td>@dr["ReadCount"]</td>
                    <td>@dr["SubTime"]</td>
                    <td>@dr["State"]</td>
                    @*<td>
                        @if (DataConverter.CStr(dr["state"]) != "已关闭")
                        {
                            <a href="javascript:;" onclick="postAPI('close','@dr["QuestionId"]');" class="btn btn-info btn-sm"><i class="fa fa-close"></i> 关闭问题</a>
                        }
                        <a href="javascript:;" onclick="postAPI('udel','@dr["QuestionId"]');" class="btn btn-danger btn-sm"><i class="fa fa-trash-o"></i> 删除</a>
                    </td>*@
                </tr>
                        }
    @Html.Partial("_page",Model)
        </table>


