@{
    
}
@section head{<title>班级浏览</title>

}
@section content{

    <div id="pageflag" data-nav="edu" data-ban="ke"></div>
	<div class="container">
	<nav aria-label="breadcrumb" role="navigation">
	<ol class="breadcrumb">
		<li class="breadcrumb-item"><a href="@Url.Action("Default","")">用户中心</a></li>
		<li class="breadcrumb-item"><a href="@Url.Action("ClassManage","Exam")">班级管理</a></li>
		<li class="breadcrumb-item active">班级浏览</li>
	</ol>
	</nav>
	</div>
    <div class="container btn_green">
        <table class="table table-striped table-bordered table-hover">
            <tr>
                <td class="w-50">班级名称:</td>
                <td><label id="classname_l"></label></td>
            </tr>
            <tr>
                <td >班标:</td>
                <td><label id="classicon_l"></label></td>
            </tr>
            <tr>
                <td>所属学校:</td>
                <td><label id="schoolname_l"></label></td>
            </tr>
            <tr>
                <td>班级年级:</td>
                <td>
                    <label id="grade_l"></label>
                </td>
            </tr>
            <tr>
                <td>班主任:</td>
                <td>
                    <label id="createuser_l"></label>
                </td>
            </tr>
            <tr>
                <td>是否毕业:</td>
                <td>
                    @*<asp:Literal ID="IsDonw_L" runat="server" EnableViewState="false"></asp:Literal>*@
                    @MvcHtmlString.Create(ViewBag.modelhtml)
                </td>
            </tr>
            <tr>
                <td>班级星级:</td>
                <td>
                    @*<asp:Literal EnableViewState="false" ID="Star_Li" runat="server"></asp:Literal>*@
                    @MvcHtmlString.Create(ViewBag.modelhtml)
                </td>
            </tr>
            <tr>
                <td>建立时间:</td>
                <td>
                    <label id="createtime_l"></label>
                </td>
            </tr>
            <tr>
                <td>班级信息:</td>
                <td>
                    <label id="classinfo_l"></label>
                </td>
            </tr>
        </table>
        <div class="text-center">
            <a href="javascript" id="edit_a" class="btn btn-info">重新修改</a>
            <a href="@Url.Action("ClassManage","Exam")" class="btn btn-outline-info">返回列表</a>
        </div>
    </div>
<script>
//导航处理
	$("#usernav1 ul li").removeClass("active").siblings().eq(3).addClass("active");
</script>	
}
@section script{

}