@section head{
    <title>优惠券激活</title>
}
@section content{
    <div id="pageflag" data-nav="group" data-ban="ArriveManage"></div>
    <div class="container">
		<nav aria-label="breadcrumb" role="navigation">
		<ol class="breadcrumb">
		<li class="breadcrumb-item"><a title="会员中心" href="/User/Default">会员中心</a></li>
		<li class="active breadcrumb-item">优惠券激活</li>
		</ol>
		</nav>

        <form method="post" action="Arrive_Act">
            <table class="table table-striped table-bordered table-hover">
                <tr>
                    <td colspan="2" class="text-center">优惠劵激活</td>
                </tr>
                <tr>
                    <td class="td_l">
                        优惠券编号：
                    </td>
                    <td>
                        <input type="text" class="border form-control text_300 required" title="优惠券编号不能为空" id="ANo" />
                    </td>
                </tr>
                <tr>
                    <td>优惠券密码：</td>
                    <td>
                        <input type="text" class="form-control text_300 required" title="优惠券密码不能为空" id="APwd" />
                    </td>
                </tr>
                <tr>
                    <td></td>
                    <td>
                        <input type="submit" class="btn btn-primary" value="激活优惠券" />
                    </td>
                </tr>
            </table>
        </form>
    </div>
}
@section script{
    <script>
        $(function () {
            $("form").validate();
			$("#usernav1 ul li").removeClass("active").siblings().eq(2).addClass("active");
        });
    </script>
}
